     RowBox[{"Select", "[", 
      RowBox[{"c", ",", 
       RowBox[{
        RowBox[{"StringMatchQ", "[", 
         RowBox[{"#", ",", 
          RowBox[{
           RowBox[{"CharacterRange", "[", 
            RowBox[{"\"\<A\>\"", ",", "\"\<z\>\""}], "]"}], ".."}]}], "]"}], 
        "&"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{"tennames", " ", "=", " ", 
    RowBox[{"Take", "[", 
     RowBox[{
      RowBox[{"normalizeText", "@", "chemicalNames"}], ",", "10"}], 
     "]"}]}]}]}]], "Code",
