Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"normalizeDataset", "[", "s_", "]"}], " ", ":=", " ", 
    RowBox[{"ToLowerCase", " ", "@", " ", 
     RowBox[{"RemoveDiacritics", " ", "@", " ", 
      RowBox[{"StringReplace", "[", 
       RowBox[{"s", ",", "\n", "\t", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"WordBoundary", "~~", 
            RowBox[{"(", 
             RowBox[{"WordCharacter", ".."}], ")"}], "~~", "\"\<.\>\""}], 
           "\[RuleDelayed]", "\"\<\>\""}], ",", 
          RowBox[{"(*", 
           RowBox[{
            RowBox[{"Remove", " ", "\"\<mr.\>\""}], ",", "\"\<jr.\>\"", ",", 
            "..."}], "*)"}], "\n", "\t", 
          RowBox[{
           RowBox[{"\"\<\[Mars]\>\"", "|", "\"\<\[Venus]\>\""}], "\[Rule]", 
           "\"\<\>\""}], ",", 
          RowBox[{"(*", 
           RowBox[{"Remove", " ", "gender", " ", "hints"}], "*)"}], "\n", 
          "\t", 
          RowBox[{
           RowBox[{"\"\<\[Hyphen]\>\"", "|", "\"\<-\>\"", "|", "\"\<'\>\""}], 
           "\[Rule]", "\"\< \>\""}], ",", 
          RowBox[{"(*", 
           RowBox[{"Remove", " ", "very", " ", "rare", " ", "characters"}], 
           "*)"}], "\n", "\t", 
          RowBox[{"DigitCharacter", "\[Rule]", "\"\<\>\""}], " ", ",", 
          RowBox[{"(*", 
           RowBox[{"Remove", " ", "very", " ", "rare", " ", "characters"}], 
           "*)"}], "\n", "\t", 
          RowBox[{
           RowBox[{"\"\<(\>\"", "|", "\"\<)\>\""}], " ", "\[Rule]", " ", 
           "\"\<\>\""}], ",", 
          RowBox[{"(*", 
           RowBox[{"Remove", " ", "Brackets"}], "*)"}], "\n", "\t", 
          RowBox[{
           RowBox[{"\"\<%\>\"", "|", "\"\<:\>\""}], " ", "\[Rule]", " ", 
           "\"\<\>\""}]}], "}"}]}], 
       RowBox[{"(*", 
        RowBox[{"Remove", " ", "Colons", " ", "and", " ", "%"}], "*)"}], 
       "]"}]}]}]}], ";"}], "\n"}]], "Code",
